{
  "printWidth": 200,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "trailingComma": "es5",
  "useTabs": false,
  "tabWidth": 2,
  "semi": true,
  "arrowParens": "always",
  "proseWrap": "preserve",
  "singleAttributePerLine": true,
  "plugins": ["prettier-plugin-tailwindcss"],
  "tailwindFunctions": ["clsx"],
  "endOfLine": "auto"
}
